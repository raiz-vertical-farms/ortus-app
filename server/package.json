{
  "name": "app",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "npm run codegen && tsx watch ./src/index.ts",
    "build": "npm run codegen && npm run compile",
    "codegen": "npm run codegen:kysely",
    "codegen:kysely": "kysely-codegen --out-file ./src/db/types.ts --dialect sqlite",
    "compile": "pkgroll",
    "deploy": "npm run migrate && npm run build",
    "migrate": "tsx ./src/scripts/migrate.ts db.sqlite ./src/migrations"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@hono/node-server": "^1.17.1",
    "@hono/standard-validator": "^0.1.5",
    "@hono/vite-build": "^1.6.2",
    "@hono/vite-dev-server": "^0.20.0",
    "@hono/zod-validator": "^0.7.2",
    "@libsql/client": "^0.15.15",
    "@libsql/kysely-libsql": "^0.4.1",
    "@scalar/hono-api-reference": "^0.9.19",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "^9.0.10",
    "better-sqlite3": "^12.2.0",
    "croner": "^9.1.0",
    "dotenv": "^17.2.1",
    "hono": "^4.8.4",
    "hono-openapi": "^1.0.8",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.7",
    "mqtt": "^5.14.1",
    "pkgroll": "^2.15.4",
    "tsc": "^2.0.4",
    "typescript": "^5.9.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.131.47",
    "@tanstack/router-plugin": "^1.131.47",
    "@types/node": "^24.1.0",
    "kysely-codegen": "^0.19.0",
    "tsx": "^4.20.5"
  }
}
